pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                println 'hello world from Build stage'
            }
        }
        stage('Test') { 
            steps {
                println 'hello world from Test stage...'
            }
        }
        stage('Deploy') { 
            steps {
                println 'hello world from Deploy stage'
            }
        }
    }
}